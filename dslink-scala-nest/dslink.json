{
	"name":"dslink-scala-nest",
	"version":"0.1.0-SNAPSHOT",
	"description":"Nest DSLink",
	"license":"Apache",
	"author":{
		"name":"Vlad Orzhekhovskiy",
		"email":"vlad@uralian.com"
	},
	"main":"bin/dslink-scala-nest",
	"repository":{
		"type":"git",
		"url":"https://github.com/uralian/dslink-scala-nest"
	},
	"bugs":{
		"url":"https://github.com/uralian/dslink-scala-nest/issues"
	},
	"configs":{
		"name":{
			"type":"string",
			"default":"nest"
		},
		"broker":{
			"type":"url"
		},
		"token":{
			"type":"string"
		},
		"nodes":{
			"type":"path",
			"default":"nodes.json"
		},
		"key":{
			"type":"path",
			"default":".key"
		},
		"log":{
			"type":"enum",
			"default":"info"
		},
		"handler_class":{
			"type":"string",
			"default":"com.uralian.dsa.examples.DummyDSLinkHandler"
		}		
	}
}
